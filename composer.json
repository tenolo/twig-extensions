{
    "name": "tenolo/twig-extensions",
    "description": "basic twig extensions.",
    "keywords": [
        "base",
        "twig",
        "extension",
        "extensions",
        "tenolo"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "http://tenolo.de",
    "authors": [
        {
            "name": "tenolo - Robert Bernis, Johannes Herberhold GbR",
            "email": "info@tenolo.de",
            "homepage": "http://tenolo.de"
        },
        {
            "name": "Nikita Loges",
            "email": "n.loges@tenolo.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "twig/twig": "~1.20|~2.0",
        "tenolo/utilities": "~1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.tenolo.com/repo/private/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenolo\\Twig\\Extensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenolo\\Twig\\Extensions\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/0.1": "0.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
